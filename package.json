{
  "name": "program-technosphere",
  "version": "1.0.0",
  "private": true,
  "description": "this is conversation for developer",
  "homepage": ".",
  "author": "wuzhixin",
  "main": "./electron/main.js",
  "scripts": {
    "build": "umi build",
    "dev": "umi dev",
    "electorn:dev": "electron .",
    "electron:build": "pnpm run build && electron-builder",
    "electron:dev:hot": "nodemon --watch electron --exec pnpm electorn:dev",
    "postinstall": "umi setup",
    "prepare": "husky install",
    "start": "npm run dev"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,jsx,ts,tsx,css,less}": [
      "umi lint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "ahooks": "^3.5.0",
    "antd": "^4.21.3",
    "dayjs": "^1.11.3",
    "foca": "^1.0.0",
    "storejs": "^2.0.1",
    "umi": "^4.0.0",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/storejs": "^2.0.3",
    "@umijs/lint": "^4.0.1",
    "@umijs/plugins": "^4.0.0",
    "electron": "^19.0.5",
    "electron-builder": "^23.1.0",
    "eslint": "^8.18.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.2",
    "mockjs": "^1.1.0",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^2",
    "prettier-plugin-packagejson": "^2",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3",
    "typescript": "^4.1.2"
  },
  "build": {
    "appId": "org.wzx.tech",
    "productName": "技术分享圈",
    "directories": {
      "output": "electron-dist"
    },
    "mac": {
      "category": "wuzhixin.app.program.technosphere"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "extends": null
  }
}
